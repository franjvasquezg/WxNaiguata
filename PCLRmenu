                       fi
                       if [ "$vARCHISLRBPOK" = "0" ]
                       then
                          scp -B -q $vDIRSRC/$vArchBanco ${SSSH_USER}@${FTP_HOSTXCOM}:${vDIRTGT}/${vArchBanco}.FTP 2>&1 | tee -a $vAutoLog
                          vSSHStat=$?
                          if [ "$vSSHStat" != "0" ]
                             then
                             vSSHERROR=1
                          fi
